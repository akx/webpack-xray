{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "module": "es2015",
        "lib": ["esnext", "dom"],
        "allowJs": false,
        "checkJs": false,
        "declaration": false,
        "sourceMap": true,
        "downlevelIteration": true,
        "jsx": "react",
        "strict": true,
        "noImplicitAny": false,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": false,
        "moduleResolution": "node"
    }
}
